@model List<StudentModel>
@{
    int access = 0;
    int.TryParse(ViewContext.RouteData.Values["accessLevel"]?.ToString(), out access);
}


<div>
    @if(access < 2) {
        <h1 class="text-danger">You do not have a sufficient access level to view this data</h1>
    } 
    else if(access >= 2)
    {
       <table class="table table-bordered">
            <thead>
                <tr>
                    <th>First Name</th>
                    <th>Last Name</th>
                    @if (access >= 8){
                        <th>Grade</th>
                    }
                </tr>
            </thead>
            <tbody>
                @foreach(var student in Model)
                {
                    <tr>
                        <td>@student.FirstName</td>
                        <td>@student.LastName</td>
                        @if(access >= 8){
                            <td>@student.Grade</td>
                        }
                    </tr>
                }
            </tbody>

        </table>
    }
</div>
